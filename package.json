{
  "name": "lake-st-clair-fishing-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3010",
    "build": "next build",
    "start": "next start -p 3010",
    "lint": "next lint",
    "server": "node server/index.js",
    "server:dev": "set NODE_ENV=development&& node server/index.js",
    "db:migrate": "node server/migrations/migrate.js",
    "stop": "npx kill-port 3010 3011",
    "docker:check": "bash scripts/check-docker.sh",
    "services:restart": "bash scripts/restart-services.sh",
    "services:health": "netstat -ano | findstr :3010 && netstat -ano | findstr :3011 && docker ps"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.0",
    "node-cron": "^4.2.1",
    "pg": "^8.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^14.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "nodemon": "^3.0.0"
  }
}
